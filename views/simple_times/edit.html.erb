<% @page_title = _('SimpleTimes') + ': ' + h(@simple_time.user.name) -%>
<% include_simple_times_stylesheet -%>
<% slim_page -%>

<% plus_form_for [Project.current, @simple_time] do |f| -%>  
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>

  <% f.fieldset do -%>
    <%= f.submit _('Update') %> | <%= link_to _('Cancel'), project_simple_time_path(Project.current, @simple_time) %>
  <% end -%>
<% end -%>
  